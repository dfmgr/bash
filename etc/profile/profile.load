case "$(uname -s)" in
CYGWIN* | MINGW32* | MSYS* | MINGW*)
  if find "$HOME.config/shell/profile"/*.win &>/dev/null; then
    for f in "$HOME.config/shell/profile"/*.win; do
      . "$f"
    done
  elif find "$HOME.config/bash/profile"/*.win &>/dev/null; then
    for f in "$HOME.config/bash/profile"/*.win; do
      . "$f"
    done
  fi
  ;;
Darwin)
  if find "$HOME.config/shell/profile"/*.mac &>/dev/null; then
    for f in "$HOME.config/shell/profile"/*.mac; do
      . "$f"
    done
  elif find "$HOME.config/bash/profile/"*.mac &>/dev/null; then
    for f in "$HOME.config/bash/profile"/*.mac; do
      . "$f"
    done
  fi
  ;;
Linux)
  if find "$HOME.config/shell/profile"/*.lin &>/dev/null; then
    for f in "$HOME.config/shell/profile"/*.lin; do
      . "$f"
    done
  elif find "$HOME.config/bash/profile"/*.lin &>/dev/null; then
    for f in "$HOME.config/bash/profile"/*.lin; do
      . "$f"
    done
  fi
  ;;
*) ;;
esac
